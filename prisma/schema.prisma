generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//product schema
model Product {
  id          String   @id @default(cuid())
  type        String  
  license     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}